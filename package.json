    "node-sass": "^4.12.0",
    "postcss-load-config": "^2.1.0",
    "postcss-loader": ">=3.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
